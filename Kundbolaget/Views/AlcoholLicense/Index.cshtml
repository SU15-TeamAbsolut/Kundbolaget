@model IEnumerable<Kundbolaget.Models.EntityModels.Customer>

@{
    ViewBag.Title = "Index";
}

<h2>Alkohollicenser</h2>

<p>
    @Html.ActionLink("Lägg till licens", "Create", null, new { @class=" btn btn-primary"})
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AlcoholLicense.StartDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AlcoholLicense.EndDate)
        </th>
        <th>
            
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AlcoholLicense.StartDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AlcoholLicense.EndDate)
        </td>
        <td>
            @Html.ActionLink("Redigera", "Edit", new {id = item.Id})
        </td>
    </tr>
}

</table>
